{
    "label": "Blog",
    "server": "/api/v1/",
    "entities": [
        {
            "name": "post",
            "attributes": {
                "author_name": "string",
                "label": {
                    "type": "string"
                }
            }
        },
        {
            "name": "page",
            "label": "Post Page",
            "attributes": {
                "post": {
                    "type": {
                        "relation": "belongsTo",
                        "with": "post"
                    }
                },
                "position": "number",
                "label": {
                    "type": "string",
                    "defaultValue": "New page"
                },
                "content": "code"
            }
        }
    ]
}